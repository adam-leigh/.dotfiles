// Place your key bindings in this file to override the defaults
[

  {
  
  "key": "ctrl+o",
  
  "command": "projectManager.listGitProjects#sideBarGit"
  
  },
  
  {
  
  "key": "ctrl+=","command": "expand_region", "when": "editorTextFocus"
  
  },
  
  {
  
  "key": "ctrl+-","command": "undo_expand_region", "when": "editorTextFocus && editorHasSelection"
  
  },
  
  {
  
  "key": "ctrl+s",
  
  "command": "workbench.action.toggleSidebarVisibility"
  
  },
  
  {
  
  "key": "ctrl+s",
  
  "command": "workbench.files.action.focusFilesExplorer",
  
  "when": "editorTextFocus"
  
  },
  
  {
  
  "key": "n",
  
  "command": "explorer.newFile",
  
  "when": "filesExplorerFocus && !inputFocus"
  
  },
  
  {
  
  "key": "d",
  
  "command": "explorer.newFolder",
  
  "when": "filesExplorerFocus && !inputFocus"
  
  },
  
  {
  
  "key": "y",
  
  "command": "filesExplorer.copy",
  
  "when": "filesExplorerFocus && !inputFocus"
  
  },
  
  {
  
  "key": "p",
  
  "command": "filesExplorer.paste",
  
  "when": "filesExplorerFocus && !inputFocus"
  
  },
  
  {
  
  "key": "shift+d",
  
  "command": "deleteFile",
  
  "when": "filesExplorerFocus && !inputFocus"
  
  },
  
  {
  
  "key": "cmd+shift+enter",
  
  "command": "python.execInTerminal-icon"
  
  },
  
  {
  
  "key": "shift+alt+enter",
  
  "command": "interactive.acceptChanges",
  
  "when": "inlineChatHasProvider && inlineChatVisible && !inlineChatDocumentChanged || inlineChatHasProvider && inlineChatVisible && config.inlineChat.editMode != 'preview'"
  
  },
  
  {
  
  "key": "cmd+enter",
  
  "command": "-interactive.acceptChanges",
  
  "when": "inlineChatHasProvider && inlineChatVisible && !inlineChatDocumentChanged || inlineChatHasProvider && inlineChatVisible && config.inlineChat.editMode != 'preview'"
  
  },
  
  {
  
  "key": "ctrl+enter",
  
  "command": "explorer.openWith"
  
  },
  
  {
  
  "key": "ctrl+k",
  
  "command": "notebook.focusPreviousEditor",
  
  "when": "config.notebook.navigation.allowNavigateToSurroundingCells && editorTextFocus && inputFocus && notebookEditorFocused && !accessibilityModeEnabled && !isEmbeddedDiffEditor && notebookEditorCursorAtBoundary != 'bottom' && notebookEditorCursorAtBoundary != 'none'"
  
  },
  
  {
  
  "key": "alt+up",
  
  "command": "-notebook.cell.moveUp",
  
  "when": "notebookEditorFocused && !inputFocus"
  
  },
  
  {
  
  "key": "ctrl+k",
  
  "command": "notebook.focusPreviousEditor",
  
  "when": "config.notebook.navigation.allowNavigateToSurroundingCells && editorTextFocus && inputFocus && notebookEditorFocused && !accessibilityModeEnabled && !isEmbeddedDiffEditor && notebookEditorCursorAtBoundary != 'bottom' && notebookEditorCursorAtBoundary != 'none'"
  
  },
  
  {
  
  "key": "ctrl+space",
  
  "command": "workbench.action.terminal.toggleTerminal",
  
  "when": "terminal.active"
  
  },
  
  {
  
  "key": "ctrl+`",
  
  "command": "-workbench.action.terminal.toggleTerminal",
  
  "when": "terminal.active"
  
  },
]
